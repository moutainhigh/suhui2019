<mapper namespace="org.suhui.modules.suhui.suhui.mapper.PayUserLoginMapper">

    <!-- 根据用户名查询 -->
    <select id="getUserByPhone" resultType="org.suhui.modules.suhui.suhui.entity.PayUserLogin">
